{
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@public/(.*)$",
    "^@src/(.*)$",
    "^@components/(.*)$",
    "^@stores/(.*)$",
    "^@services/(.*)$",
    "^@utilities/(.*)$",
    "^@hooks/(.*)$",
    "^@assets/(.*)$",
    "^@constants/(.*)$",
    "^[./].*(?<!.scss)$",
    ".*(scss|css)$"
  ],
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}
